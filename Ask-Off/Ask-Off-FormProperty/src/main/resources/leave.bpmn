            </bpmndi:BPMNShape>
            <bpmndi:BPMNEdge bpmnElement="flow2" id="BPMNEdge_flow2">
                <omgdi:waypoint x="35.0" y="63.0"></omgdi:waypoint>
                <omgdi:waypoint x="80.0" y="63.0"></omgdi:waypoint>
